{
  "name": "@gi.ts/generator-html",
  "version": "1.5.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "export:html": "node ./_build/html/generator.js",
    "serve:html": "http-server html -p 8080 -d",
    "config-docs": "XDG_DATA_DIRS=/usr/share/:/usr/share/gnome-shell/:/usr/lib/mutter-8/ gi-ts config --lock",
    "list-docs": "gi-ts config",
    "gen-docs": "gi-ts generate --withDocs --format=json --versionedOutput --propertyCase=underscore --out=./json",
    "format": "prettier --write src/**/*.ts src/**/*.tsx",
    "prepack": "rm -rf dist && yarn build"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-syntax-highlighter": "^15.4.3",
    "rehype-highlight": "^4.1.0",
    "rehype-react": "^6.2.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-behead": "^2.3.3",
    "remark-breaks": "^2.0.2",
    "remark-fix-guillemets": "^1.1.1",
    "remark-heading-id": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "unified": "^9.2.1"
  },
  "devDependencies": {
    "@types/highlight.js": "^10.1.0",
    "@types/node": "^14.14.41",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-syntax-highlighter": "^11.0.5",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@gi.ts/lib": "^1.5.3"
  },
  "resolutions": {
    "@types/react": "16.14.5",
    "@types/react-dom": "16.9.12"
  }
}
