{
  "name": "@gi.ts/cli",
  "description": "The command line interface for gi.ts (type checking for GObject Introspection and GJS)",
  "version": "1.5.5",
  "author": "Evan Welsh",
  "bin": {
    "gi-ts": "./bin/run"
  },
  "bugs": "https://gitlab.gnome.org/ewlsh/gi.ts/issues",
  "dependencies": {
    "@gi.ts/node-loader": "^1.5.3",
    "@gi.ts/parser": "^1.5.3",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "prettier": "^2.2.1",
    "tslib": "^1"
  },
  "peerDependencies": {
    "@gi.ts/lib": "^1.5.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "@types/prettier": "^2.1.6",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "ts-node": "^8",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ewlsh/gi.ts",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gi-ts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://gitlab.gnome.org/ewlsh/gi.ts",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
