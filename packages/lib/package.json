{
  "name": "@gi.ts/lib",
  "version": "2.0.0-alpha.0",
  "main": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js"
    },
    "./generators/*": {
      "types": "./dist/generators/*",
      "import": "./dist/generators/*"
    }
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@gi.ts/parser": "workspace:*"
  },
  "devDependencies": {
    "typescript": "5.1.3"
  },
  "scripts": {
    "build": "tsc",
    "prepack": "rm -rf dist && yarn build"
  }
}
